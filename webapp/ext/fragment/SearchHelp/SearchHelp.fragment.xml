<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
	    id   ="Location"
		title="Storage Location"
		ok=".onValueHelpOkPress"
		cancel=".onValueHelpCancelPress"
		afterClose=".onValueHelpAfterClose"
		key="{fieldSearch>/header/key}"
		supportMultiselect="false">
		<filterBar>
			<fb:FilterBar id = "1" advancedMode="true" search=".onFilterBarSearch" isRunningInValueHelpDialog="true" filterGroupItems="{fieldSearch>/header/items}">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="{fieldSearch>name}"
						label="{fieldSearch>label}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input value="{fieldSearch>/header/key}"/>
						</fb:control>
					</fb:FilterGroupItem>               
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
