<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
	    id   ="SearchHelpAsset"
		title="Asset"
		ok=".onValueHelpOkPress"
		cancel=".onValueHelpCancelPress"
		afterClose=".onValueHelpAfterClose"
		key="MasterFixedAsset"
		supportMultiselect="false">
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch" isRunningInValueHelpDialog="true" >
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="CompanyCode"
						label="Company Code"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="CompanyCode" />
						</fb:control>
					</fb:FilterGroupItem>                
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="MasterFixedAsset"
						label="Master Fixed Asset"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="MasterFixedAsset" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="FixedAssetExternalID"
						label="Fixed Asset External ID"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="FixedAssetExternalID" />
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
