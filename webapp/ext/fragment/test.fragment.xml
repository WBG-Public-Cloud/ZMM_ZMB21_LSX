<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="GL Account"
		ok=".onValueHelpOkPress"
		cancel=".onValueHelpCancelPress"
		afterClose=".onValueHelpAfterClose"
		key="GLAccount">
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch" isRunningInValueHelpDialog="true">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="CompanyCode"
						label="Company Code"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="CompanyCode" />
						</fb:control>
					</fb:FilterGroupItem>                
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="GLAccount"
						label="GLAccount"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="GLAccount" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="GLAccountName"
						label="GLAccount Name"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="GLAccountName" />
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
